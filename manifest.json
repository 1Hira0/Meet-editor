{
	"manifest_version": 2,

	"name": "Meet editor",
	"description": "For now just opens meet",
	"version": "1.3.0",
	"background": {
		"scripts": ["background.js"]
	},
	"content_scripts": [
		{
			"matches": ["https://meet.google.com/*"],
			"js": ["myscript.js"]
		}
	],

	"permissions": ["tabs", "https://meet.google.com/*"],
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "My Sample Extension"
	}
}
